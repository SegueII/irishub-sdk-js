{
  "name": "@irisnet/sdk-js",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha",
    "build": "rm -rf lib && npx babel src -d lib",
    "build:docs": "npx jsdoc-to-markdown -f src/modules/**/*.js > docs/api.md",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "events": "^3.0.0",
    "url": "^0.11.0",
    "pumpify": "^1.5.1",
    "websocket-stream": "^5.5.0",
    "ndjson": "^1.5.0",
    "camelcase": "^5.3.1",
    "iris-crypto": "git+https://github.com/irisnet/irisnet-crypto.git#develop"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "jsdoc-to-markdown": "^4.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "files": [
    "lib/*"
  ]
}
